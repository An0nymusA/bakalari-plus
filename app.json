{
  "expo": {
    "name": "Bakalari+",
    "slug": "bakalariplus",
    "version": "1.0.0",
    "icon": "./src/assets/icons/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/icons/splash.png",
      "backgroundColor": "#141518"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icons/adaptive-icon.png",
        "backgroundColor": "#111411"
      },
      "package": "com.anonymous.bakalariplus",
      "allowBackup": false
    },
    "androidStatusBar": {
      "backgroundColor": "#141518",
      "barStyle": "light-content"
    },
    "web": {
      "favicon": "./src/assets/icons/favicon.png",
      "bundler": "metro"
    },
    "scheme": "bakalariplus",
    "experiments": {
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d3ed06bb-f358-40b8-a978-17172a788662",
        "image": "latest"
      }
    }
  }
}
